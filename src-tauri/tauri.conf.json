{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/y-32x32.png",
      "icons/y-128x128.png",
      "icons/y-128x128@2x.png",
      "icons/y-icon.icns",
      "icons/y-icon.ico"
    ]
  },
  "productName": "y-clip",
  "version": "0.2.0",
  "identifier": "com.tauri.build",
  "plugins": {},
  "app": {
    "trayIcon": {
      "id": "sys-tray",
      "iconPath": "icons/y-icon.png",
      "showMenuOnLeftClick": true,
      "iconAsTemplate": true
    },
    "windows": [
      {
        "label": "main",
        "title": "Y-CLIP",
        "url": "/main",
        "decorations": false,
        "fullscreen": false,
        "resizable": false,
        "hiddenTitle": true,
        "alwaysOnTop": true,
        "visible": false,
        "transparent": true,
        "skipTaskbar": true,
        "shadow": false,
        "minimizable": true
      },
      {
        "label": "config",
        "url": "/config",
        "title": "Y-ClIP Configuration",
        "visible": true,
        "width": 648.0,
        "height": 480.0,
        "decorations": true,
        "fullscreen": false,
        "resizable": true,
        "hiddenTitle": false,
        "alwaysOnTop": false,
        "transparent": false,
        "useHttpsScheme": true,
        "minWidth": 648.0,
        "maxWidth": 940.0,
        "minHeight": 480.0,
        "maxHeight": 810
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  }
}
